<UserControl x:Class="Drugs2020.PL.Views.Login"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Drugs2020.PL.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel HorizontalAlignment="Left" Height="227" Margin="336,84,0,0" VerticalAlignment="Top" Width="299">
        <TextBlock TextWrapping="Wrap" Text="User ID:"/>
        <TextBox Height="23" TextWrapping="Wrap" Text="TextBox" TextChanged="TextBox_TextChanged"/>
        
        <TextBlock TextWrapping="Wrap" Text="Password"/>
        <TextBox Height="23" TextWrapping="Wrap" Text="TextBox"/>
        <Button Content="Log in" Margin="0,0,0.2,0"/>
        <Button Content="Enter as Administrator" Margin="0,0,0.2,0"/>
    </StackPanel>

</UserControl>
